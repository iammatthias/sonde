{
  "name": "sonde",
  "main": "dist/_worker.js/index.js",
  "compatibility_date": "2025-10-30",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "rules": [
    {
      "type": "Data",
      "globs": ["**/*.ttf", "**/*.woff2"],
      "fallthrough": true
    }
  ]
}
